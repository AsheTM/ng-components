  "license": "MIT",
  "scripts": {
    "build:ng-dialog": "npm run lint:ng-dialog && npm run test:ng-dialog && nx build ng-dialog",
    "build:ng-wallet-components": "npm run lint:ng-wallet-components && npm run test:ng-wallet-components && nx build ng-wallet-components",
    "lint:ng-dialog": "nx lint ng-dialog --format stylish",
    "lint:ng-wallet-components": "nx lint ng-wallet-components --format stylish",
    "lint:ng-dialog:fix": "npm run lint:ng-dialog -- --fix",
    "lint:ng-wallet-components:fix": "npm run lint:ng-wallet-components -- --fix",
    "package:ng-dialog": "npm run build:ng-dialog && cd ./dist/packages/ng-dialog && npm pack",
    "package:ng-wallet-components": "npm run build:ng-wallet-components && cd ./dist/packages/ng-wallet-components && npm pack",
    "publish:ng-dialog": "cd ./dist/packages/ng-dialog && npm publish --access=public",
    "publish:ng-wallet-components": "cd ./dist/packages/ng-wallet-components && npm publish --access=public",
    "test:ng-dialog": "nx test ng-dialog --browsers ChromeHeadless --watch false",
    "test:ng-wallet-components": "nx test ng-wallet-components --browsers ChromeHeadless --watch false"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@nrwl/angular": "^14.5.4",
    "@nrwl/cli": "13.1.4",
    "@nrwl/eslint-plugin-nx": "13.1.4",
    "@nrwl/jest": "13.1.4",
    "@nrwl/linter": "13.1.4",
    "@nrwl/tao": "13.1.4",
    "@nrwl/workspace": "13.1.4",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "ng-packagr": "^14.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0"
  }
}

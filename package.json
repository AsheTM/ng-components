  "scripts": {
    "build:ng-components-demo": "npm run lint:ng-components-demo & npm run test:ng-components-demo & ng build ng-components-demo",
    "build:ng-dialog": "npm run lint:ng-dialog & npm run test:ng-dialog & ng build ng-dialog",
    "lint:ng-components-demo": "ng lint ng-components-demo --format stylish",
    "lint:ng-components-demo:fix": "npm run lint:ng-components-demo -- --fix",
    "lint:ng-dialog": "ng lint ng-dialog --format stylish",
    "lint:ng-dialog:fix": "npm run lint:ng-dialog -- --fix",
    "package:ng-dialog": "npm run build:ng-dialog && cd ./dist/ng-dialog && npm pack",
    "publish:ng-dialog": "cd ./dist/ng-dialog && npm publish --access=public",
    "start:ng-components-demo": "ng serve ng-components-demo -- --open",
    "test:ng-components-demo": "ng test ng-components-demo --browsers ChromeHeadless --watch false",
    "test:ng-dialog": "ng test ng-dialog --browsers ChromeHeadless --watch false"
  },
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "~14.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@schematics/angular": "^11.2.19",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
